{
  "name": "leadrsagents",
  "version": "1.0.0",
  "description": "Agent OS - Multi-Agent System for Leadrs",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "leadrs": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "cd web && pnpm dev",
    "start": "cd web && pnpm start",
    "build": "tsc && cd web && pnpm build",
    "build:production": "pnpm build && echo 'Build completed! Deploy the web/.next folder'",
    "cli": "tsx src/cli/index.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts && cd web && pnpm lint",
    "typecheck": "tsc --noEmit && cd web && pnpm run typecheck",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "clean": "rm -rf web/.next web/node_modules/.cache dist"
  },
  "dependencies": {
    "@fastify/cookie": "^10.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/websocket": "^11.0.0",
    "@google/genai": "^1.0.0",
    "@supabase/supabase-js": "^2.45.0",
    "apify-client": "^2.9.0",
    "commander": "^12.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "glob": "^11.0.0",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.75.0",
    "pino": "^9.5.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.16.0",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
